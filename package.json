    "js-yaml": "^4.1.0",
    "nanoid": "^5.0.9",
    "pino": "^9.6.0",
    "prando": "^6.0.1",
    "pure-rand": "^6.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-to-zod": "^3.15.0",
    "yaml": "^2.7.0",
    "zod": "^3.24.1",
    "zod-to-json-schema": "^3.24.1"
